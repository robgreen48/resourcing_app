<div class="panel panel-default" id="month_view">
  <div class="panel-heading"><h3><%= Date::MONTHNAMES[@view.month] %>, <%= @view.year %></h3></div>
  <div class="panel-body"><p data-no-turbolink><%= link_to '<< PREVIOUS MONTH', decrement_month_view_path, :class => "btn btn-default btn-sm", :id => "month_view_buttons" %> | <%= link_to 'NEXT MONTH >>', increment_month_view_path, :class => "btn btn-default btn-sm", :id => "month_view_buttons" %></p></div>
</div>